# Supabase Configuration
# Copy this file to .env.local and fill in your Supabase credentials

# Get these from: https://supabase.com/dashboard/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Optional: Google AI (Already configured)
GOOGLE_AI_API_KEY=your_existing_key_here

# ElevenLabs TTS Configuration (for voice responses)
# Get API key from: https://elevenlabs.io/app/settings/api-keys
NEXT_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
# Voice ID is hardcoded to LISA (Z3R5wn05IrDiVCyEkUrK) - professional, clear voice for financial assistant

# Replicate API Configuration (alternative TTS provider)
# Get API token from: https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Stripe Configuration
# Get these from: https://dashboard.stripe.com/apikeys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs
# Create products in Stripe Dashboard and get their Price IDs (e.g., price_...)
STRIPE_BASIC_PRICE_ID=price_...
STRIPE_PRO_PRICE_ID=price_...
STRIPE_UNLIMITED_PRICE_ID=price_...
STRIPE_LIFETIME_PRICE_ID=price_...

# App URL for redirects
NEXT_PUBLIC_URL=http://localhost:3000
